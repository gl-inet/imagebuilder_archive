			select PACKAGE_boost-coroutine
			default n

		config boost-graph-parallel
			bool "Boost parallel graph support."
			select PACKAGE_boost-graph
			default n

		 config PACKAGE_boost-atomic
			prompt "Boost atomic library."
			default m if ALL
			 

		  config PACKAGE_boost-chrono
